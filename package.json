{
  "name": "react-audio-player-v2",
  "version": "1.0.0",
  "description": "Simple audio player built with React.js (for educational purposes)",
  "main": "index.js",
  "scripts": {
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "lint:css": "jest --config test/jest.stylelint.js",
    "lint": "jest --config test/jest.eslint.js",
    "start": "npm run dev",
    "dev": "webpack-dev-server --mode=development",
    "build": "webpack --mode=production",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(ts|tsx|js|json|css|html|md)\""
  },
  "author": "Alexander-Pomazan",
  "license": "ISC",
  "dependencies": {
    "@types/styled-components": "^5.1.0",
    "jest-styled-components": "^7.0.2",
    "jotai": "^0.7.1",
    "linaria": "^1.3.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.1.0",
    "styled-reset": "^4.1.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.+(ts|tsx|js|json|css|html|md)": [
      "prettier --ignore-path .gitignore --write",
      "jest --findRelatedTests"
    ]
  },
  "jest-runner-eslint": {
    "cliOptions": {
      "ignorePath": "./.gitignore",
      "fix": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^8.3.4",
    "@hot-loader/react-dom": "^16.13.0",
    "@testing-library/jest-dom": "^5.7.0",
    "@testing-library/react": "^10.0.4",
    "@testing-library/user-event": "^10.3.1",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.0.1",
    "core-js": "^3.6.5",
    "css-loader": "^3.5.3",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-jest": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.2",
    "fork-ts-checker-webpack-plugin": "^4.1.4",
    "get-audio-duration": "^1.0.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "is-ci-cli": "^2.0.0",
    "jest": "^26.0.1",
    "jest-runner-eslint": "^0.8.0",
    "jest-runner-stylelint": "^2.3.3",
    "jest-watch-select-projects": "^2.0.0",
    "jest-watch-typeahead": "^0.6.0",
    "lint-staged": "^10.2.4",
    "mini-css-extract-plugin": "^0.9.0",
    "prettier": "^2.0.5",
    "react-hot-loader": "^4.12.21",
    "serve": "^11.3.0",
    "stylelint": "^13.4.1",
    "stylelint-config-recommended": "^3.0.0",
    "typescript": "^4.0.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  }
}
